    <sqlMap resource="sqlmaps/ngp/AdTrack.xml"/>
    <sqlMap resource="sqlmaps/ngp/App.xml"/>
    <sqlMap resource="sqlmaps/ngp/AppConfig.xml"/>
    <sqlMap resource="sqlmaps/ngp/AppData.xml"/>
    <sqlMap resource="sqlmaps/ngp/AppVersion.xml"/>
    <sqlMap resource="sqlmaps/ngp/AppZone.xml"/>
    <sqlMap resource="sqlmaps/ngp/CoinHistory.xml"/>
    <sqlMap resource="sqlmaps/ngp/Device.xml"/>
    <sqlMap resource="sqlmaps/ngp/EngineNode.xml"/>
    <sqlMap resource="sqlmaps/ngp/FileStorage.xml"/>
    <sqlMap resource="sqlmaps/ngp/IosOrder.xml"/>
    <sqlMap resource="sqlmaps/ngp/Message.xml"/>
    <sqlMap resource="sqlmaps/ngp/Order.xml"/>
    <sqlMap resource="sqlmaps/ngp/PaypalIndent.xml"/>
    <sqlMap resource="sqlmaps/ngp/PaypalOrder.xml"/>
    <sqlMap resource="sqlmaps/ngp/Placard.xml"/>
    <sqlMap resource="sqlmaps/ngp/PlacardTarget.xml"/>
    <sqlMap resource="sqlmaps/ngp/Promo.xml"/>
    <sqlMap resource="sqlmaps/ngp/Product.xml"/>
    <sqlMap resource="sqlmaps/ngp/PromoCount.xml"/>
    <sqlMap resource="sqlmaps/ngp/PushToken.xml"/>
    <sqlMap resource="sqlmaps/ngp/ResUpdate.xml"/>
    <sqlMap resource="sqlmaps/ngp/ResUpdateFile.xml"/>
    <sqlMap resource="sqlmaps/ngp/Sale.xml"/>
    <sqlMap resource="sqlmaps/ngp/SaleHistory.xml"/>
    <sqlMap resource="sqlmaps/ngp/SnsData.xml"/>
    <sqlMap resource="sqlmaps/ngp/SnsFriend.xml"/>
    <sqlMap resource="sqlmaps/ngp/User.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserAccount.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserActivity.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserActivityHistory.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserApp.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserAppData.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserFriend.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserKey.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserProfile.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserRole.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserSession.xml"/>
    <sqlMap resource="sqlmaps/ngp/UserSessionCount.xml"/>
